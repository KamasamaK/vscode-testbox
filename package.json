{
    "name": "vscode-testbox",
    "displayName": "vscode-testbox",
    "description": "TestBox Support for Visual Studio Code",
    "version": "0.1.0",
    "publisher": "ortus-solutions",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:lang-cfml",
        "onLanguage:cfml",
        "workspaceContains:**/*.cfm",
        "workspaceContains:**/*.cfml",
        "workspaceContains:**/*.cfc"
    ],
    "contributes": {
        "snippets": [{
                "language": "lang-cfml",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "cfml",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "keywords": [
        "CFML",
        "ColdFusion",
        "Lucee",
        "TestBox",
        "MockBox",
        "BDD",
        "TDD"
    ],
    "dependencies": {}
}